[package]
name = {{crateStrings.name}}
version = {{crateStrings.version}}
{{#if crateStrings.description}}
description = {{crateStrings.description}}
{{/if}}
{{#if crateStrings.author}}
authors = [{{crateStrings.author}}]
{{/if}}
{{#if crateStrings.license}}
license = {{crateStrings.license}}
{{/if}}
edition = "2021"
exclude = ["index.node"]

[lib]
crate-type = ["cdylib"]

# See more keys and their definitions at https://doc.rust-lang.org/cargo/reference/manifest.html

[dependencies]
neon = "{{versions.neon}}"
